</head>
<body>
    <div class="container-fluid">
        <table class="table">
            <tr>
                <td>Tên của sinh viên</td>
                <td>Email của sinh viên nhá</td>
                <td>Orther</td>
            </tr>
            <tr>
                <% for(let i=0;i<data.lenght;i++){ %>
                    <td><%=data[i].firstname%></td>
                    <td><%=data[i].email%></td>
                    <td>
                        <a href="xoa">Delete</a>
                        ||
                        <a href="update">Update</a>
                    </td>
                <% } %>
            </tr>
        </table>
    </div>  
</body>
</html>